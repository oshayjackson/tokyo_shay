      "settings": {
        "foreground": "#288BD2"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#6D71C3"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
